<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.example2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.PizzaApp.infoForCourier"></activity>
        <activity android:name="com.example.PizzaApp.CityForOrder" />
        <activity android:name="com.example.PizzaApp.OrderFinish" />
        <activity android:name="com.example.PizzaApp.OrderPizza" />
        <activity android:name="com.example.PizzaApp.PizzaDescription" />
        <activity android:name="com.example.PizzaApp.PizzaMeniu" />
        <activity android:name="com.example.PizzaApp.PizzaPlaces" />
        <activity android:name="com.example.PizzaApp.Register" />
        <activity android:name="com.example.PizzaApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>